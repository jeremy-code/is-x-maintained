    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier 'src/**/*.{js,jsx,ts,tsx,json}'",
    "format:fix": "prettier 'src/**/*.{js,jsx,ts,tsx,json}' --write"
